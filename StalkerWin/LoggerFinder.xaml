        <TextBox Name="lab_log" HorizontalAlignment="Stretch" Foreground="Red" Margin="45,10,0,0" VerticalAlignment="Stretch"/>
        <Image Name="img_anm" HorizontalAlignment="Left" VerticalAlignment="Top" Width="28" Height="26" Margin="10,10,0,0"/>
    </Grid>
</Window>
